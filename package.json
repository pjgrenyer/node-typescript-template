{
    "name": "node-typescript-template",
    "version": "1.0.1",
    "description": "A template node typescript project",
    "main": "index.js",
    "scripts": {
        "prepare": "husky install",
        "build": "rimraf dist && tsc",
        "dev": "npm run build && node dist/index.js",
        "test": "jest --coverage",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' './*.js' './*.json' '!./package-lock.json'",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pjgrenyer/node-typescript-template.git"
    },
    "keywords": [
        "node",
        "typescript"
    ],
    "author": "Paul Grenyer",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/pjgrenyer/node-typescript-template/issues"
    },
    "homepage": "https://github.com/pjgrenyer/node-typescript-template#readme",
    "dependencies": {
        "typescript": "^5.0.4"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.21.4",
        "@babel/preset-typescript": "^7.21.4",
        "@types/jest": "^29.5.1",
        "@types/supertest": "^2.0.12",
        "@typescript-eslint/eslint-plugin": "^5.59.1",
        "@typescript-eslint/parser": "^5.59.1",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^8.39.0",
        "husky": "^8.0.3",
        "jest": "^29.5.0",
        "jest-mock-extended": "^3.0.4",
        "nock": "^13.3.1",
        "prettier": "^2.8.8",
        "supertest": "^6.3.3"
    }
}
