{
    "name": "node-typescript-template",
    "version": "1.1.2",
    "description": "A template node typescript project",
    "main": "index.js",
    "engines": {
        "npm": ">=9.8.1",
        "node": ">=v20.5.1"
    },
    "scripts": {
        "prepare": "husky install",
        "build": "rimraf dist && tsc",
        "dev": "npm run build && node dist/index.js",
        "test": "jest --coverage",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' './*.js' './*.json' '!./package-lock.json'",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pjgrenyer/node-typescript-template.git"
    },
    "keywords": [
        "node",
        "typescript"
    ],
    "author": "Paul Grenyer",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/pjgrenyer/node-typescript-template/issues"
    },
    "homepage": "https://github.com/pjgrenyer/node-typescript-template#readme",
    "dependencies": {
        "typescript": "^5.5.3"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.24.7",
        "@babel/preset-typescript": "^7.24.7",
        "@types/jest": "^29.5.12",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.16.0",
        "@typescript-eslint/parser": "^7.16.0",
        "babel-cli": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "eslint": "^8.56.0",
        "husky": "^9.0.11",
        "jest": "^29.7.0",
        "jest-mock-extended": "^3.0.7",
        "nock": "^13.5.4",
        "prettier": "^3.3.2",
        "supertest": "^7.0.0"
    }
}
