{
    "name": "node-typescript-template",
    "version": "1.1.2",
    "description": "A template node typescript project",
    "main": "index.js",
    "engines": {
        "npm": ">=9.8.1",
        "node": ">=v20.5.1"
    },
    "scripts": {
        "prepare": "husky",
        "build": "rimraf dist && tsc",
        "dev": "npm run build && node dist/index.js",
        "test": "jest --coverage",
        "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts' './*.js' './*.json' '!./package-lock.json'",
        "lint": "eslint . --ext .ts"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/pjgrenyer/node-typescript-template.git"
    },
    "keywords": [
        "node",
        "typescript"
    ],
    "author": "Paul Grenyer",
    "license": "SEE LICENSE IN LICENSE",
    "bugs": {
        "url": "https://github.com/pjgrenyer/node-typescript-template/issues"
    },
    "homepage": "https://github.com/pjgrenyer/node-typescript-template#readme",
    "dependencies": {
        "typescript": "^5.9.2"
    },
    "devDependencies": {
        "@babel/preset-env": "^7.28.3",
        "@babel/preset-typescript": "^7.27.1",
        "@babel/traverse": "^7.28.4",
        "@types/jest": "^30.0.0",
        "@types/supertest": "^6.0.3",
        "babel-cli": "^6.26.0",
        "braces": "^3.0.3",
        "eslint": "^9.35.0",
        "husky": "^9.1.7",
        "jest": "^30.1.3",
        "jest-mock-extended": "^4.0.0",
        "json5": "^2.2.3",
        "nock": "^14.0.10",
        "prettier": "^3.6.2",
        "rimraf": "^6.0.1",
        "supertest": "^7.1.4",
        "ts-node": "^10.9.2",
        "typescript-eslint": "^8.43.0"
    }
}
